node('master'){
    stage('vcs'){
        git 'https://github.com/maheshmakham/game-of-life.git'
    }
    stage('build'){
        sh label: '', script: 'mvn package'
    }
    stage('post-build'){
        archive 'gameoflife-web/target/*.jar'
        junit 'gameoflife-web/target/surefire-reports/*.xml'
    }
}